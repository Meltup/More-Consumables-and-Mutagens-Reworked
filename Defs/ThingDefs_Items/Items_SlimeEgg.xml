  <RecipeDef>
    <defName>MakeCatalystSerumRed</defName>
    <label>make catalyst serum (red)</label>
    <description>Make red mutagenic catalyst serum from medical supplies and raw meat.</description>
    <jobString>Making red catalyst.</jobString>
    <workAmount>700</workAmount>
    <workSpeedStat>DrugCookingSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_CookMeal</soundWorking>
	<allowMixingIngredients>true</allowMixingIngredients>
    <ingredients>
      <li>
        <filter>
          <categories>
            <li>MeatRaw</li>
          </categories>
        </filter>
        <count>16</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>Neutroamine</li>
          </thingDefs>
        </filter>
        <count>2</count>
      </li>
	  <li>
        <filter>
          <thingDefs>
            <li>MedicineIndustrial</li>
          </thingDefs>
        </filter>
        <count>2</count>
      </li>
    </ingredients>
    <products>
      <CatalystSerumRed>1</CatalystSerumRed>
    </products>
    <fixedIngredientFilter>
      <categories>
        <li>MeatRaw</li>
      </categories>
      <thingDefs>
        <li>MedicineIndustrial</li>
        <li>Neutroamine</li>
      </thingDefs>
    </fixedIngredientFilter>
    <defaultIngredientFilter>
      <categories>
        <li>MeatRaw</li>
      </categories>
      <thingDefs>
        <li>MedicineIndustrial</li>
        <li>Neutroamine</li>
      </thingDefs>
    </defaultIngredientFilter>
    <workSkill>Intellectual</workSkill>
	<recipeUsers>
      <li>DrugLab</li>
    </recipeUsers>
    <researchPrerequisite>MutagenOne</researchPrerequisite>
    <skillRequirements>
      <Medicine>6</Medicine>
      <Cooking>4</Cooking>
    </skillRequirements>
  </RecipeDef>
  
